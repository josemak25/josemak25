{
  "name": "peaky-sneakers-server",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start:prod": "cross-env NODE_ENV=production && node dist/index.js",
    "start:dev": "yarn format && nodemon dist/index.js",
    "start:debug": "cross-env DEBUG=express-mongoose-es6-rest-api:* yarn start",
    "format": "prettier --write \"**/*.{js, jsx, ts, tsx, json}\"",
    "test": "cross-env NODE_ENV=test && jest --runInBand --verbose --detectOpenHandles --forceExit",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "yarn test --coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "keywords": [
    "online-shop",
    "shoes",
    "tracker",
    "resource manager",
    "e-commerce"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josemak25/hi-chat-app-backend.git"
  },
  "homepage": "https://github.com/josemak25/hi-chat-app-backend#readme",
  "bugs": {
    "url": "https://github.com/josemak25/hi-chat-app-backend/issues"
  },
